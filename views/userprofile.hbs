            <!-- Orders Table-->
            <div class="col-lg-8 pb-5">
                <div class="d-flex justify-content-end pb-3">
                    <div class="form-inline">
                        <label class="text-muted mr-3" for="order-sort">Sort Orders</label>
                        <select class="form-control" id="order-sort">
                            <option>All</option>
                            <option>Delivered</option>
                            <option>In Progress</option>
                            <option>Delayed</option>
                            <option>Canceled</option>
                        </select>
                    </div>
                </div>
                <div class="table-responsive">
                    <table class="table table-hover mb-0">
                        <thead>
                            <tr>
                                <th>Order ID</th>
								<th>User ID</th>
								<th>Item ID</th>
								<th>PG ID</th>
								<th>PG Name</th>
								<th>Order Item</th>
                                <th>Quantity</th>
                                
                                <th>Status</th>
                                
                            </tr>
                        </thead>
                        <tbody>
                        {{#if msg}}
                        {{#each data}}
                        <tr>
                            {{!-- <th scope="row">{{@index}}</th> --}}
                            <td>{{this.order_id}}</td>
                            <td>{{this.user_id}}</td>
                            <td>{{this.item_id}}</td>
                            <td>{{this.pg_id}}</td>
                            <td>{{this.pg_name}}</td>
                            <td>{{this.order_item}}</td>
                            <td>1</td>
                            {{!-- //<td>{{this.order_date}}</td> --}}
                            <td>{{this.delivery_status}}</td>
                            
                            
                        </tr>
                        {{/each}}
                       {{/if}}
                        </tbody>
                    </table>
                        {{!-- {{else}}
