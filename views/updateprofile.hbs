			  <div class="form-wrapper is-active" >
				<button type="button" class="switcher">
				  Update Information
				  <span class="underline"></span>
				</button>
				<form class="form user-signup" action="/updateinfo" onsubmit="return ValidateForm()" >
				  <fieldset>
					<!-- <legend>Please, enter your email and password for login.</legend> -->
					<div class="column">
						<div class="input-block">
							<label for="user-name">Name</label>
							<input id="name" name="name" type="text" value={{name}} readonly >
						  </div>
						  <div class="input-block">
							<label for="user-street">Street name</label>
							<input id="street" name="street" type="text" value={{street}}>
						  </div>
						  <div class="input-block">
							<label for="user-pincode">Pincode</label>
							<input id="pincode" name="pincode" type="text" value={{pincode}} >
						  </div>
						  <div class="input-block">
							<label for="user-email">E-mail</label>
							  <input id="email" name="email" type="email" value={{email}} readonly>
						  </div>
						 
							
					</div>
					<div class="column">
						<!-- <div class="input-block">
							<label for="user-lname">Last name</label>
							<input id="user-lname" name="lname" type="text" >
						  </div> -->
						  <div class="input-block">
							<label for="user-house">House no</label>
							<input id="house" name="house" type="text" value="{{house}}">
						  </div>
						  <div class="input-block">
							<label for="user-phone">Phone Number</label>
							<input id="phone" name="phone" type="phone" value="{{phone}}">
						  </div>
						  
							<div class="input-block">
								<label for="user-password">Password</label>
							<input id="password" name="password" type="text" value="{{password}}" readonly>
							  </div>
							  <div class="input-block">
								<label for="user-password">New Password</label>
							<input id="password1" name="password1" type="password" >
							  </div>
					</div>
	
				  </fieldset>
				  <button type="submit" class="btn-signup" >Update</button>
				 
				</form>
			</div>
